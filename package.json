  "workspaces": [
    "repositories/dockers/*",
    "repositories/frontends/example-frontends/*",
    "repositories/frontends/other-frontends/*",
    "repositories/monorepo-libraries/*",
    "repositories/packages/next-packages/*",
    "repositories/packages/other-packages/*",
    "repositories/packages/react-packages/*",
    "repositories/packages/rust-packages/*",
    "repositories/packages/vanilla-js-packages/*"
