    "all": "yarn workspaces foreach -A run",
    "all:del:dist": "yarn all del:dist",
    "all:del:node_modules": "yarn all del:node_modules",
    "all:ncu:update": "yarn all ncu:update",
    "all:pack:build": "yarn all pack:build",
    "all:typecheck": "yarn all typecheck",
    "monorepo-libraries:build": "yarn workspaces foreach -A --include 'repositories/monorepo-libraries/*' run build",
    "webs:typecheck": "yarn workspaces foreach -A --include 'src/webs/*' run typecheck",
    "pack:build:step1": "yarn workspaces foreach -A --include @wisdomstar94/vanilla-js-util run pack:build",
    "pack:build:step2": "yarn workspaces foreach -A --include @wisdomstar94/ip-utils --include @wisdomstar94/react-add-event-listener --include @wisdomstar94/react-body --include @wisdomstar94/react-debounce --include @wisdomstar94/react-promise-interval --include @wisdomstar94/react-promise-timeout --include @wisdomstar94/react-indexeddb-manager --include @wisdomstar94/react-request-animation-frame-manager --include @wisdomstar94/react-scroll-controller run pack:build",
