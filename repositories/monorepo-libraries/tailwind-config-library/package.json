{
  "name": "@wisdomstar94/tailwind-config-library",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc-watch",
    "ncu:update": "ncu -u --dep \"prod\",\"dev\",\"optional\",\"packageManager\",\"peer\""
  },
  "type": "module",
  "files": [
    "./src/*"
  ],
  "exports": {
    "./configs/*": {
      "default": "./src/configs/*"
    }
  },
  "peerDependencies": {
    "tailwindcss": "^4.0.2"
  },
  "devDependencies": {
    "tailwindcss": "^4.0.2",
    "typescript": "^5.7.3"
  }
}
