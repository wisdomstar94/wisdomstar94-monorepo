{
  "name": "@wisdomstar94/react-web-rtc-manager",
  "version": "0.0.3",
  "license": "MIT",
  "scripts": {
    "ncu:update": "ncu -u --dep \"prod\",\"dev\",\"optional\",\"packageManager\",\"peer\"",
    "pack:build": "vite build && tsc",
    "npm:publish": "npm publish --access=public",
    "del:dist": "del-cli ./dist",
    "del:node_modules": "del-cli ./node_modules",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@wisdomstar94/tsconfig-library": "workspace:^",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "files": [
    "package.json",
    "dist/*",
    "!dist/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  }
}
