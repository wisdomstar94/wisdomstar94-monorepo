{
  "name": "@wisdomstar94/babylon-js-example-socket",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc -p ./tsconfig.express.json",
    "watch": "tsc-watch -p ./tsconfig.express.json",
    "ncu:update": "ncu -u",
    "nodemon:start": "nodemon",
    "dev": "yarn build && concurrently \"yarn nodemon:start\" \"yarn watch\""
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv-mono": "^1.3.14",
    "express": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@wisdomstar94/react-babylon-utils": "workspace:^",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "npm-check-updates": "^17.1.11",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2"
  }
}
